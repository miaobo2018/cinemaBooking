<% if (user == 'none') { %>
    <input id="name" placeholder="Choose name"/>

    <input id="email" placeholder="Choose mail"/>
    <input id="phone" placeholder="Choose phone number"/>
<% } else { %>
    <input id="name" value='<%= user.name %>'/>

    <input id="email" value='<%= user.email %>'/>
    <input id="phone" value='<%= user.cellphone %>'/>
<% } %>